{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@images/*": ["./src/assets/images/*"],
      "#/*": ["./src/components/*"],
      "##/*": ["./src/components/Pages/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
